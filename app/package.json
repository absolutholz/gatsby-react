{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "develop": "run-p gatsby-develop storybook-develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "gatsby-build": "gatsby build",
    "gatsby-clean": "gatsby clean",
    "gatsby-develop": "gatsby develop -H 0.0.0.0 -p 8000",
    "gatsby-serve": "gatsby serve",
    "storybook-develop": "start-storybook -p 6006 -s ./public",
    "storybook-build": "build-storybook"
  },
  "dependencies": {
    "gatsby": "^3.1.2",
    "gatsby-plugin-sass": "^4.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@storybook/addon-actions": "^6.2.2",
    "@storybook/addon-essentials": "^6.2.2",
    "@storybook/addon-links": "^6.2.2",
    "@storybook/builder-webpack5": "^6.2.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
